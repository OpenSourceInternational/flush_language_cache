{
	"name": "cobweb/flush_language_cache",
	"type": "typo3-cms-extension",
	"description": "Adds an item to the flush cache menu to flush just the language (l10n) cache.",
	"license": [
		"GPL-2.0+"
	],
	"authors": [
		{
			"name": "Francois Suter",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"Cobweb\\FlushLanguageCache\\": "Classes/"
		}
	},
	"version": "2.0.0",
	"require": {
		"typo3/cms-core": ">=8.7.0,<9.0"
	},
	"replace": {
		"flush_language_cache": "self.version",
		"typo3-ter/flush_language_cache": "self.version"
	}
}
